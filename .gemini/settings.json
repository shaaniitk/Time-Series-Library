{
  "theme": "GitHub",
  "selectedAuthType": "gemini-api-key",
  "mcpServers": {
		"sequentialthinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"type": "stdio"
		},
		"huggingface": {
			"type": "http",
			"url": "https://hf.co/mcp",
			"gallery": true
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"/Users/shantanumisra/workspace/"
			],
			"type": "stdio"
		},
		"zen": {
			"command": "uv",
			"args": [
				"--directory",
				"/Users/shantanumisra/workspace/zen-mcp-server",
				"run",
				"server.py"
			]
		},
		"serena": {
			"command": "uv",
			"args": [
				"--directory",
				"/Users/shantanumisra/workspace/serena",
				"run",
				"serena-mcp-server"
			]
		},
		"memory": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory@latest"
			],
			"env": {
				"MEMORY_FILE_PATH": "/Users/shantanumisra/workspace/MCP/memort-bank/memory.json"
			},
			"autoApprove": [
			"memory_bank_read",
			"memory_bank_write",
			"memory_bank_update",
			"list_projects",
			"list_project_files"
			],
			"gallery": true
		},
		 "smart-tree": {
					"command": "st",
					"args": ["--mcp"],
					"env": {},
					"mode": "ai",
					"stream": true,
						"compress": true
					}
	}
  }

  
