{
  "theme": "GitHub",
  "selectedAuthType": "gemini-api-key",
  "mcpServers": {
		"sequentialthinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"type": "stdio"
		},
		"huggingface": {
			"type": "http",
			"url": "https://hf.co/mcp",
			"gallery": true
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"D:\\workspace"
			],
			"type": "stdio"
		},
		"zen": {
			"command": "C:\\Users\\mishr\\.local\\bin\\uv.exe",
			"args": [
				"--directory",
				"D:\\workspace\\zen-mcp-server",
				"run",
				"server.py"
			]
		},
		"serena": {
			"command": "C:\\Users\\mishr\\.local\\bin\\uv.exe",
			"args": [
				"--directory",
				"D:\\workspace\\serena",
				"run",
				"serena-mcp-server"
			]
		},
		"desktop-commander": {
			"command": "npx",
			"args": [
				"-y",
				"@wonderwhy-er/desktop-commander"
			],
			"type": "stdio"
		},
		"memory": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory@latest"
			],
			"env": {
				"MEMORY_FILE_PATH": "D:\\workspace\\memory_banks"
			},
			"autoApprove": [
			"memory_bank_read",
			"memory_bank_write",
			"memory_bank_update",
			"list_projects",
			"list_project_files"
			],
			"gallery": true
		}
	}
  }

  
